    "version": "0.2.0",
    "configurations": [
        {
            "name": "streamclienttest",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/streamclienttest",
            "args": [
                "--request",
                "read",
                "--server",
                "tcp://127.0.0.1:12345",
                "--config",
                "defaultconfig.cf",
                "--dir",
                "../temp/CN-Educoder/fifth"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "rpcclienttest",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/rpcclienttest",
            "args": [
                "--server",
                "--remote",
                "tcp://127.0.0.1:1234",
                "--file",
                "temp.txt"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/iofwd/iofwd",
            "args": [
                "--config",
                "defaultconfig.cf"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
